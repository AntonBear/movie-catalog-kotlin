<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_nav"
    app:startDestination="@id/fragment_feed_screen">
    <fragment
        android:id="@+id/fragment_feed_screen"
        android:name="com.anton.movie_catalog_kotlin.feedScreenFragment.FeedScreenFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_feed_screen">
        <action
            android:id="@+id/action_feed_screen_to_movie_details_screen"
            app:destination="@id/movie_details_screen" />

    </fragment>

    <fragment
        android:id="@+id/fragment_movies_screen"
        android:name="com.anton.movie_catalog_kotlin.MoviesScreenFragment"
        android:label="fragment_movies_screen"
        tools:layout="@layout/fragment_movies_screen" />

    <composable
        android:id="@+id/movie_details_screen"
        android:name="com.anton.movie_catalog_kotlin.movieDetailsScreen.MovieDetailsScreenKt\$MovieDetailsScreen" />

    <composable
        android:id="@+id/compose_favorites_fragment"
        android:name="com.anton.movie_catalog_kotlin.favoritesScreen.FavoritesScreenKt\$FavoritesScreen" />

    <fragment
        android:id="@+id/fragment_profile_screen"
        android:name="com.anton.movie_catalog_kotlin.ProfileScreenFragment"
        android:label="fragment_profile_screen"
        tools:layout="@layout/fragment_profile_screen" />
</navigation>